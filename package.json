{
  "name": "mvt-viernes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./build && tsc",
    "dev": "tsnd ./src/modules/index.ts",
    "test": "jest --verbose",
    "test-c": "jest --coverage --runInBand",
    "test-v": "jest --verbose --runInBand --detectOpenHandles",
    "test-d": "jest --testNamePattern='describe' --verbose",
    "test-f": "jest --testPathPatterns='Environment.test.ts' --verbose",
    "test:e2e": "jest --verbose test/e2e",
    "test:e2e:puppeteer": "jest --testMatch \"**/test/e2e/**/*.puppeteer.test.ts\" --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "serve-favicon": "^2.5.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.8.0",
    "@types/serve-favicon": "^2.5.7",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "puppeteer": "^24.28.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
